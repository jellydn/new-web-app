{
  "version": "0.4.0",
  "commands": {
    "react": {
      "id": "react",
      "description": "React App Generator",
      "strict": true,
      "pluginName": "new-web-app",
      "pluginAlias": "new-web-app",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "$ npx new-web-app@latest react -n=react-app -a=yes -q=yes",
        "$ npx new-web-app@latest react --name=react-app --airbnb=yes --react-query=yes"
      ],
      "flags": {
        "version": {
          "name": "version",
          "type": "boolean",
          "char": "v",
          "description": "Show CLI version.",
          "allowNo": false
        },
        "help": {
          "name": "help",
          "type": "boolean",
          "char": "h",
          "description": "Show CLI help.",
          "allowNo": false
        },
        "name": {
          "name": "name",
          "type": "option",
          "char": "n",
          "description": "folder name to create",
          "multiple": false
        },
        "tailwind": {
          "name": "tailwind",
          "type": "option",
          "char": "t",
          "description": "add tailwind css",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        },
        "storybook": {
          "name": "storybook",
          "type": "option",
          "char": "s",
          "description": "add storybook",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        },
        "linter": {
          "name": "linter",
          "type": "option",
          "char": "a",
          "description": "add ESLint, Prettier with sort imports plugin",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        },
        "react-query": {
          "name": "react-query",
          "type": "option",
          "char": "q",
          "description": "add react-query",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        },
        "react-hook-form": {
          "name": "react-hook-form",
          "type": "option",
          "char": "f",
          "description": "add react-hook-form",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        },
        "cypress": {
          "name": "cypress",
          "type": "option",
          "char": "c",
          "description": "add cypress",
          "multiple": false,
          "options": [
            "yes",
            "no"
          ]
        }
      },
      "args": {}
    },
    "react:upgrade-react-18": {
      "id": "react:upgrade-react-18",
      "description": "Upgrade to React 18",
      "strict": true,
      "pluginName": "new-web-app",
      "pluginAlias": "new-web-app",
      "pluginType": "core",
      "aliases": [],
      "flags": {
        "directory": {
          "name": "directory",
          "type": "option",
          "char": "d",
          "description": "folder name to update",
          "multiple": false
        }
      },
      "args": {}
    }
  }
}