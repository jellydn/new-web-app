{
  "name": "new-web-app",
  "version": "0.3.2",
  "description": "Frontend app generator",
  "keywords": [
    "new-web-app",
    "new web app",
    "web app",
    "frontend generator",
    "app generator",
    "react app generator"
  ],
  "homepage": "https://github.com/jellydn/new-web-app",
  "bugs": "https://github.com/jellydn/new-web-app/issues",
  "repository": "jellydn/new-web-app",
  "license": "MIT",
  "author": "Huynh Duc Dung @jellydn",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "dist/index.d.ts",
  "bin": {
    "new-web-app": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "husky install",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "vercel-build": "cd website && yarn install && yarn build",
    "version": "oclif readme && git add README.md"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "dependencies": {
    "@oclif/core": "^1.8.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@oclif/plugin-update": "^3.0.0",
    "degit": "^2.8.4",
    "execa": "5.1.1",
    "replace-in-file": "^6.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@swc/core": "1.2.182",
    "@swc/helpers": "0.3.13",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/degit": "2.8.3",
    "@types/node": "16.11.34",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-productsway": "0.1.14",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "oclif": "3.0.1",
    "prettier": "2.6.2",
    "regenerator-runtime": "^0.13.9",
    "shx": "0.3.4",
    "ts-node": "10.7.0",
    "tslib": "2.4.0",
    "tsup": "5.12.7",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "oclif": {
    "bin": "new-web-app",
    "dirname": "new-web-app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "react": {
        "description": "Create a new React app with ViteJs"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "minify": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  }
}
