{
  "name": "new-web-app",
  "description": "Frontend app generator",
  "version": "0.3.0",
  "author": "Huynh Duc Dung @jellydn",
  "bin": {
    "new-web-app": "./bin/run"
  },
  "bugs": "https://github.com/jellydn/new-web-app/issues",
  "dependencies": {
    "@oclif/core": "^1.6.4",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "degit": "^2.8.4",
    "execa": "5.1.1",
    "replace-in-file": "^6.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@swc/core": "1.2.168",
    "@swc/helpers": "^0.3.8",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/degit": "2.8.3",
    "@types/node": "17.0.23",
    "@types/shelljs": "0.8.11",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.13.0",
    "eslint-config-productsway": "0.1.14",
    "husky": ">=7.0.4",
    "lint-staged": "12.3.8",
    "oclif": "3.0.1",
    "prettier": "2.6.2",
    "regenerator-runtime": "^0.13.9",
    "shx": "0.3.4",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "tsup": "5.12.5",
    "typescript": "4.7.0-dev.20220416"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jellydn/new-web-app",
  "keywords": [
    "new-web-app",
    "new web app",
    "web app",
    "frontend generator",
    "app generator",
    "react app generator"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "oclif": {
    "bin": "new-web-app",
    "dirname": "new-web-app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "react": {
        "description": "Create a new React app with ViteJs"
      }
    }
  },
  "repository": "jellydn/new-web-app",
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "vercel-build": "cd website && yarn install && yarn build",
    "prepare": "husky install"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "minify": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
